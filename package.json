{
  "name": "dalelarroder.com",
  "version": "2.0.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "postbuild": "yarn sitemap && yarn rss",
    "sitemap": "cross-env NODE_OPTIONS='--experimental-json-modules' node ./scripts/generate-sitemap.mjs",
    "rss": "cross-env NODE_OPTIONS='--experimental-json-modules' node ./scripts/generate-rss.mjs",
    "serve": "next start",
    "analyze": "cross-env ANALYZE=true next build",
    "lint": "next lint --fix --dir pages --dir components --dir lib --dir layouts --dir scripts",
    "prepare": "husky install"
  },
  "dependencies": {
    "@fontsource/mukta": "4.5.9",
    "classnames": "2.3.1",
    "contentlayer": "0.2.7",
    "esbuild": "0.14.47",
    "framer-motion": "7.1.0",
    "github-slugger": "1.4.0",
    "logrocket": "3.0.1",
    "mdx-bundler": "9.0.1",
    "next": "12.2.5",
    "next-contentlayer": "0.2.7",
    "next-themes": "0.2.0",
    "nprogress": "0.2.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-icons": "4.4.0",
    "reading-time": "1.5.0",
    "rehype-autolink-headings": "6.1.1",
    "rehype-prism-plus": "1.4.2",
    "rehype-slug": "5.0.1",
    "remark": "14.0.2",
    "swr": "1.3.0"
  },
  "devDependencies": {
    "@next/bundle-analyzer": "12.2.5",
    "@svgr/webpack": "6.3.1",
    "@tailwindcss/forms": "0.5.2",
    "@tailwindcss/typography": "0.5.4",
    "@types/github-slugger": "^1.3.0",
    "@types/node": "18.0.0",
    "@types/nprogress": "^0.2.0",
    "@types/react": "18.0.17",
    "@typescript-eslint/eslint-plugin": "5.33.1",
    "@typescript-eslint/parser": "5.33.1",
    "autoprefixer": "10.4.8",
    "cross-env": "7.0.3",
    "dedent": "0.7.0",
    "eslint": "8.20.0",
    "eslint-config-next": "12.2.5",
    "eslint-config-prettier": "8.5.0",
    "eslint-plugin-jsx-a11y": "6.6.1",
    "eslint-plugin-prettier": "4.2.1",
    "file-loader": "6.2.0",
    "globby": "11.1.0",
    "husky": "8.0.1",
    "inquirer": "9.1.0",
    "lint-staged": "13.0.3",
    "postcss": "8.4.16",
    "prettier": "2.7.1",
    "tailwindcss": "3.1.8",
    "typescript": "4.8.3"
  },
  "lint-staged": {
    "*.+(js|jsx|ts|tsx)": [
      "eslint --fix"
    ],
    "*.+(js|jsx|ts|tsx|json|css|md|mdx)": [
      "prettier --write"
    ]
  }
}
