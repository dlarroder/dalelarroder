{
  "name": "dalelarroder.com",
  "version": "1.3.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "postbuild": "yarn sitemap && yarn rss",
    "sitemap": "cross-env NODE_OPTIONS='--experimental-json-modules' node ./scripts/generate-sitemap.mjs",
    "rss": "cross-env NODE_OPTIONS='--experimental-json-modules' node ./scripts/generate-rss.mjs",
    "serve": "next start",
    "analyze": "cross-env ANALYZE=true next build",
    "lint": "next lint --fix --dir pages --dir components --dir lib --dir layouts --dir scripts",
    "prepare": "husky install"
  },
  "dependencies": {
    "@fontsource/dm-sans": "^4.5.7",
    "@fontsource/merriweather": "^4.5.12",
    "@mailchimp/mailchimp_marketing": "3.0.75",
    "contentlayer": "^0.2.5",
    "esbuild": "0.14.47",
    "framer-motion": "6.3.16",
    "github-slugger": "1.4.0",
    "gray-matter": "4.0.3",
    "image-size": "1.0.1",
    "logrocket": "3.0.1",
    "mdx-bundler": "9.0.1",
    "next": "12.1.6",
    "next-contentlayer": "^0.2.5",
    "next-themes": "0.2.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-icons": "4.4.0",
    "reading-time": "1.5.0",
    "swr": "1.3.0"
  },
  "devDependencies": {
    "@next/bundle-analyzer": "12.1.6",
    "@svgr/webpack": "6.2.1",
    "@tailwindcss/forms": "0.5.2",
    "@tailwindcss/typography": "0.5.2",
    "@types/node": "18.0.0",
    "@types/react": "18.0.14",
    "@typescript-eslint/eslint-plugin": "5.30.0",
    "@typescript-eslint/parser": "5.30.0",
    "autoprefixer": "10.4.7",
    "cross-env": "7.0.3",
    "dedent": "0.7.0",
    "eslint": "8.18.0",
    "eslint-config-next": "12.1.6",
    "eslint-config-prettier": "8.5.0",
    "eslint-plugin-jsx-a11y": "6.6.0",
    "eslint-plugin-prettier": "4.0.0",
    "file-loader": "6.2.0",
    "globby": "11.1.0",
    "husky": "8.0.1",
    "inquirer": "9.0.0",
    "lint-staged": "13.0.3",
    "postcss": "8.4.14",
    "prettier": "2.7.1",
    "tailwindcss": "3.1.4",
    "typescript": "4.7.4"
  },
  "lint-staged": {
    "*.+(js|jsx|ts|tsx)": [
      "eslint --fix"
    ],
    "*.+(js|jsx|ts|tsx|json|css|md|mdx)": [
      "prettier --write"
    ]
  }
}
